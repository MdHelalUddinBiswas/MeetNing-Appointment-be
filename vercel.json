{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "(.*)", "dest": "index.js" },
    { "src": "/api/(.*)", "dest": "/index.js" }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "env": {
    "NODE_ENV": "production",
    "PORT": "8080"
  }
}